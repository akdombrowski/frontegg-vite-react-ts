{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "arrowParens": "always",
  "endOfLine": "auto",
  "experimentalTernaries": true,
  "printWidth": 100,
  "singleAttributePerLine": true,
  "proseWrap": "preserve",
  "quoteProps": "consistent",
  "jsxSingleQuote": false,
   "plugins": ["prettier-plugin-sh"]
}
